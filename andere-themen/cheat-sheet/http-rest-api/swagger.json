        },
        "password": {
          "type": "string",
          "description": "das Passwort des Benutzers"
        }
      }
    },
    "MessagebusPublication": {
      "description": "Eine Nachricht, die auf dem MessageBus veröffentlicht werden kann",
      "properties": {
        "channel": {
          "type": "string",
          "description": "Der Kanal, auf dem die Nachricht veröffentlicht wird"
        },
        "msg": {
          "description": "Der Inhalt ihrer Nachricht",
          "type": "object"
        },
        "options": {
          "description": "Zusätzliche Abfrageoptionen für die Nachrichtenveröffentlichung",
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "applicationId": {
              "type": "string"
            },
            "token": {
              "type": "string"
            }
          }
        }
      }
    },
    "StartProcessMessage": {
      "description": "Die Nachricht um einen BPMN-Prozess zu starten",
      "properties": {
        "msg": {
          "properties": {
            "key": {
              "type": "string"
            }
          }
        },
        "options": {
          "description": "Zusätzliche Abfrageoptionen für den Prozess, der gestartet werden soll",
          "type": "object",
